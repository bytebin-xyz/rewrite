{
  "name": "bytebin",
  "version": "1.0.0",
  "scripts": {
    "build": "run-s clean:dist compile:client compile:server copy:client copy:config",
    "clean:dist": "rimraf dist",
    "compile:client": "cross-env mode=production NODE_ENV=production nuxt-ts build",
    "compile:server": "nest build",
    "copy:client": "copyfiles -a \"client/**/*\" dist",
    "copy:.nuxt": "copyfiles -a \".nuxt/**/*\" dist",
    "copy:config": "copyfiles nuxt.config.ts dist",
    "lint": "eslint \"{client,common,server,test}/**/*.ts\" --fix",
    "format": "prettier --write \"server/**/*.ts\" \"client/**/*\" \"test/**/*.ts\"",
    "start": "cross-env mode=production NODE_ENV=production node ./dist/server/main.js",
    "start:dev": "cross-env mode=development NODE_ENV=development nodemon",
    "start:dev:client": "cross-env mode=development nuxt",
    "start:dev:server": "cross-env NODE_ENV=development nodemon"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.1.3",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.1.3",
    "@nestjs/mongoose": "^7.0.1",
    "@nestjs/platform-express": "^7.1.3",
    "@nestjs/websockets": "^7.2.0",
    "@nuxt/components": "^0.3.4",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/recaptcha": "^0.6.2",
    "@saeris/vue-spinners": "^1.0.8",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "connect-mongo": "^3.2.0",
    "ejs": "^3.1.3",
    "focus-visible": "^5.1.0",
    "hamburgers": "^1.1.3",
    "helmet": "^3.23.1",
    "mjml": "^4.6.3",
    "mongoose": "^5.9.19",
    "ms": "^2.1.2",
    "nestjs-session": "^1.0.1",
    "nestjs-throttler": "^0.2.2",
    "nodemailer": "^6.4.10",
    "nuxt": "^2.12.2",
    "nuxt-property-decorator": "^2.7.2",
    "nuxt-typed-vuex": "^0.1.19",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "vue-click-outside": "^1.1.0",
    "vuelidate": "^0.7.5",
    "vuelidate-property-decorators": "^1.0.28"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.2.0",
    "@nestjs/schematics": "^7.0.1",
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/tailwindcss": "^2.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/hapi__joi": "^17.1.2",
    "@types/helmet": "^0.0.47",
    "@types/mjml": "^4.0.4",
    "@types/mongoose": "^5.7.27",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.0.13",
    "@types/nodemailer": "^6.4.0",
    "@types/request-ip": "^0.0.35",
    "@types/vuelidate": "^0.7.13",
    "copyfiles": "^2.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  }
}

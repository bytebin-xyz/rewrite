{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": ["esnext", "esnext.asynciterable", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "sourceMap": true,
    "strict": true,
    "target": "es2018",
    "typeRoots": ["types"],
    "types": ["@types/node", "@nuxt/types", "@nuxtjs/recaptcha", "@nuxtjs/toast", "vue-js-modal"]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
